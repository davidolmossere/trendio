<h2><%= video.setTitle %></h2>
<img width="100" height="150" src="<%= video.thumbnailPath %>" />
<div>Creator: <%= video.creator.name %></div>
<div>Created at: <%= video.createdAt %></div>
<div>Description: <%= video.description %></div>

<a href="/videos/<%= video.id %>/edit">Edit</a>
<%- include('../partials/deleteForm', {url: `/videos/${video.id}` }) %>
<a href="/creators/<%= video.creator.id %>">View creator</a>