<div class="content-header-block">
    <h2>Videos</h2>
    <form action="/videos" method="GET">
        <div class="form-row form-row-bundle">
            <div class="form-item">
                <input type="text" placeholder="enter a video name" name="name" value="<%= searchOptions.name %>">
            </div>
            <button class="btn btn-secondary" type="submit">Search</button>
        </div>
    </form>
</div>
<div class="content-block">
    <% videos.forEach(video => { %>
        <a href="/videos/<%= video.id %>">
            <div><%= video.setTitle %></div>
        </a>
    <% }); %>
</div>
<div class="new-item">
    <a href="/videos/new">
        <i class="fas fa-plus-circle"></i> add a video
    </a>
</div>