<h2>Edit creator</h2>
<div class="content-block">
    <form action="/creators/<%= creator.id %>?_method=PUT" method="POST" enctype="multipart/form-data">
        <%- include('_form_fields') %>
        <div class="form-row form-row-end btn-row">
            <a class="btn btn-secondary" href="/creators">Cancel</a>
            <button class="btn btn-primary" type="submit">Update</button>
        </div>
    </form>
</div>